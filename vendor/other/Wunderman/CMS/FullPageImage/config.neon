services:
	privateModuleService:
	 	class: Wunderman\CMS\FullPageImage\PrivateModule\Service

	fullPageImageFactory:
		class: App\PublicModule\Component\FullPageImage
		implement: App\PublicModule\Component\IFullPageImageFactory